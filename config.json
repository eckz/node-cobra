{
	"modules": ["file", "indexes", "autoindex", "access", "redirect", "cgi"],
	"ports": [8080],
	"documentRoot": ".",
	"virtualhosts": {
		"localhost" : {
			"locations": {
				"/": {
					"allow": "all"
				}
			}
		},
		"*": {
		}
	},
	"locations": {
		"/": {
			"autoIndex": true
		},
		"/denied/": {
			"deny": "all"
		},
		"/google": {
			"redirect": "http://google.com"
		}
	},
	"directories": {
		".": {
			"allow": "127.0.0.1",
			"deny": "all"
		},
		"./php/": {
			"handler": "cgi",
			"cgiScript": "php5-cgi"
		}
	}
}