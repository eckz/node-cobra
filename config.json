{
	"modules": ["file", "autoindex", "access"],
	"ports": [8080],
	"documentRoot": ".",
	"virtualhosts": {
		"localhost" : {
			"locations": {
				"/": {
					"allow": "all"
				}
			}
		},
		"*": {
			"deny": "all"
		}
	},
	"locations": {
		"/": {
			"autoIndex": true
		},
		"/denied/": {
			"deny": "all"
		}
	},
	"directories": {
		".": {
			"allow": "127.0.0.1",
			"deny": "all"
		}
	}
}